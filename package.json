{
  "name": "@blarestew/site",
  "private": true,
  "version": "0.0.0",
  "description": "blarestew website",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rimraf dist && parcel build --public-url ./ src/index.html",
    "deploy": "ghpages blare-stew/site --path=dist",
    "predeploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blare-stew/site.git"
  },
  "author": "David Konsumer <konsumer@jetboystudio.com> (http://konsumer.js.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blare-stew/site/issues"
  },
  "homepage": "https://blarestew.com",
  "devDependencies": {
    "cssnano": "latest",
    "ghpages": "latest",
    "node-sass": "latest",
    "parcel-bundler": "latest",
    "parcel-plugin-markdown-string": "^1.4.0",
    "parcel-plugin-prerender": "^1.4.1",
    "parcel-plugin-static-files-copy": "latest",
    "rimraf": "latest"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-markdown": "^4.2.2",
    "simple-react-router": "0.0.17",
    "three": "latest"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "articles/assets",
        "staticOutDir": "assets"
      }
    ]
  },
  "prerender": {
    "routes": ["/", "/about", "/merch", "/reviews", "/space", "/video"]
  }
}
